{
	"name": "Thorbis Default Theme",
	"version": "1.0.1",
	"description": "A clean, modern default theme for Thorbis CMS",
	"components": {
		"Header": {
			"id": "header",
			"name": "Header Component",
			"path": "/components/Header"
		},
		"Footer": {
			"id": "footer",
			"name": "Footer Component",
			"path": "/components/Footer"
		},
		"Hero": {
			"id": "hero",
			"name": "Hero Component",
			"path": "/components/Hero"
		}
	},
	"layouts": {
		"default": {
			"id": "default",
			"name": "Default Layout",
			"path": "/layouts/Default"
		}
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tailwindcss": "^3.3.0",
		"@heroicons/react": "^2.0.18"
	}
}
